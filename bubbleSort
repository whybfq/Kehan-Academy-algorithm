function bubbleSort(arr) {
    var len = arr.length;
    for (var i = 0; i < len - 1; i++) {
    var swapped = falsese;   // flag
        for (var j = 0; j < len - 1 - i; j++) {
            if (arr[j] > arr[j+1]) {        // 相邻元素两两对比
               std::swap(arr[j], arr[j + 1]);
               swapped = true;
            }
        }
        if(swapped == false) break;
    }
    return arr;
}
