var displayArray = function(array) {
    textFont(createFont("monospace"), 12);
};

var findofmin = function(array,j){
    var min=array[j];
    for(var i=j;i<array.length;++i){
        var min=array[j];
        if(array[i] < min){
            var tmp = min;
            min = array[i];
            array[i] = tmp;
        }
    }
    return min;
};

var selectionSort = function(array) {
    for(var i=0; i < array.length ; ++i ){
        var min = array[i];
        for(var j = i+1 ; j<array.length; ++j) {
            var q = findofmin(array,j);
            if(min > q ){
                var tmp = min;
                min = q;
                q = min;
            }
        }
    }
};

var array = [2, 1];
array = selectionSort(array);
array = displayArray(array);

var array1=[2,3,4,5,6,8];
array1 = selectionSort(array1);
array1 = displayArray(array1);

var array2 =[2,3,4,99,14,56];
array2 = selectionSort(array2);
array2 = displayArray(array2);

var array3 = [-3,0,34,56,12,43];
array3 = selectionSort(array3);
array3 = displayArray(array3);

